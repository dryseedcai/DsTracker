<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dryseed.timecostimpl">

    <!-- To store the time-cost info. -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application>
        <activity
            android:name="com.dryseed.timecost.ui.TimeCostInfoListActivity"
            android:enabled="false"
            android:icon="@drawable/time_cost_canary_icon"
            android:label="@string/time_cost_canary_detail_activity_label"
            android:taskAffinity="com.dryseed.timecostimpl">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>
